<odoo>

  <record id = "student_view_tree" model = "ir.ui.view">
  	<field name = "name">student tree</field>
  	<field name = "model">student</field>
  	<field name = "arch" type = "xml">
  		<tree>
  			<field name = "name"/>
  			<field name = "student_id"/>
  			<field name = "college_id"/>
  			<field name = "phone"/>
  			<field name = "email"/>
  			<field name = "academiclevel_id"/>
  			<field name = "gender"/>
  		</tree>
    </field>
  </record>	

  <record id = "student_view_form" model = "ir.ui.view">
    <field name = "name">student form</field>
    <field name = "model">student</field>
    <field name = "arch" type = "xml">
      <form>
        <sheet>
            <group>
            <field name = "name"/>
  			<field name = "student_id"/>
  			<field name = "college_id"/>
  			<field name = "department_id"/>
  			<field name = "phone"/>
  			<field name = "email"/>
  			<field name = "academiclevel_id"/> 
  			<field name = "course_ids" widget = "many2many_tags"/> 
  			<field name = "gender"/>
  			<field name = "enrollment_date"/>
            </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id = "student_view_search" model = "ir.ui.view">
    <field name = "name">student search</field>
    <field name = "model">student</field>
    <field name = "arch" type = "xml">
      <search>
            <field name = "name"/>
  			<field name = "student_id"/>
  			<field name = "college_id"/>

        <filter name ="gender" string = "Female" domain="[('gender','=','female')]"/>
        <filter name ="gender" string = "Male" domain="[('gender','=','male')]"/>
        <filter name ="group_college" string = "College" context="{'group_by':'college_id'}"/>
        <filter name ="group_academiclevel" string = "Academic Level" context="{'group_by':'academiclevel_id'}"/>
      </search>
    </field>
  </record>  

  <record id = "student_action" model = "ir.actions.act_window">
    <field name = "name">Students</field>
    <field name = "res_model">student</field>
  </record> 
 
  <menuitem
  id = "student_menu"
  name = "Students"
  parent = "students_menu"
  action = "student_action"
  />  

</odoo>